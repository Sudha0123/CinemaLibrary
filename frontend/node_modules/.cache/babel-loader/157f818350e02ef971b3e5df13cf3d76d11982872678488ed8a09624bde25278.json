{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";const initialState={MyVideoLibrary:[],videosCount:0};const likeSlice=createSlice({name:'MyLibrary',initialState,reducers:{addToLibrary(state,action){state.MyVideoLibrary.push(action.payload);state.videosCount=state.MyVideoLibrary.length;},removeFromLibrary(state,action){state.MyVideoLibrary=state.MyVideoLibrary.filter(video=>video.VideoId!==action.payload);state.videosCount=state.MyVideoLibrary.length;}}});export const{addToLibrary,removeFromLibrary}=likeSlice.actions;export default likeSlice.reducer;","map":{"version":3,"names":["createSlice","initialState","MyVideoLibrary","videosCount","likeSlice","name","reducers","addToLibrary","state","action","push","payload","length","removeFromLibrary","filter","video","VideoId","actions","reducer"],"sources":["D:/React/cinemalibrary/frontend/src/slicers/like-slicer.jsx"],"sourcesContent":["import { createSlice } from \"@reduxjs/toolkit\";\r\n\r\nconst initialState = {\r\n    MyVideoLibrary : [],\r\n    videosCount: 0\r\n}\r\nconst likeSlice = createSlice({\r\n    name : 'MyLibrary',\r\n    initialState,\r\n    reducers: {\r\n        addToLibrary(state, action){\r\n            state.MyVideoLibrary.push(action.payload);\r\n            state.videosCount = state.MyVideoLibrary.length;\r\n        },\r\n          removeFromLibrary(state, action) {\r\n      state.MyVideoLibrary = state.MyVideoLibrary.filter(video => video.VideoId !== action.payload);\r\n      state.videosCount = state.MyVideoLibrary.length;\r\n    }\r\n    }\r\n})\r\nexport const { addToLibrary ,removeFromLibrary} = likeSlice.actions;\r\nexport default likeSlice.reducer;"],"mappings":"AAAA,OAASA,WAAW,KAAQ,kBAAkB,CAE9C,KAAM,CAAAC,YAAY,CAAG,CACjBC,cAAc,CAAG,EAAE,CACnBC,WAAW,CAAE,CACjB,CAAC,CACD,KAAM,CAAAC,SAAS,CAAGJ,WAAW,CAAC,CAC1BK,IAAI,CAAG,WAAW,CAClBJ,YAAY,CACZK,QAAQ,CAAE,CACNC,YAAYA,CAACC,KAAK,CAAEC,MAAM,CAAC,CACvBD,KAAK,CAACN,cAAc,CAACQ,IAAI,CAACD,MAAM,CAACE,OAAO,CAAC,CACzCH,KAAK,CAACL,WAAW,CAAGK,